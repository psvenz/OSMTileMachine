						line-width : 0.1
						line-opacity : 0
						border-color : #caffa9
						border-style : solid
						border-opacity : 1
						border-width : 10:4;15:6;18:12
					draw : line
					define
						min-zoom : 9
						max-zoom : 11.9
						line-opacity : 1
						painting-cycle : 1000
						line-color : #4444cc
						line-width : 2
						border-style : none
					draw : line
					define
						min-zoom : 12
						max-zoom : 25
						line-color : #1fdd00
						line-width : 10:1;15:1
						line-opacity : 1
						border-style : none	
						painting-cycle : 624
					draw : line
				elsefor : @isOneOf(mtb:scale,1-,1,1+)
