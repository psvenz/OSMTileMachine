			define
				line-color : #b07f3e
				line-width : 10:0.5;15:4;18:12
				line-style : solid
				min-zoom : 10
				line-opacity : 10:0;13:1
				border-style : solid
				border-color : #000000
				border-width : 25%
				border-opacity : 10:0;13:1
				painting-cycle : 650
		elsefor : highway=secondary and maxspeed <60
			define
				min-zoom : 7
				line-color : #eebb55
				line-width : 7:0.5;13:5;18:20
				border-style : solid
				border-color : #000000
				border-width : 20%
				painting-cycle : 680
		elsefor : highway=secondary and width >10
			define
				min-zoom : 5
				line-color : #ff9155
				line-width : 6:1;11:4;17:10
				border-style : solid
				border-color : #000000
				border-width : 20%
				painting-cycle : 690
