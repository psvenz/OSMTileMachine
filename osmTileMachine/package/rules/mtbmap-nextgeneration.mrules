// Example of Maperitive rendering rules for OSMTileMachine



features
	points
		place : place
		named_points : shop or restaurant or pub or natural=peak
	lines
		road : highway
		railway : railway=rail 
		bridge : bridge=yes or bridge=viaduct
		waterway : waterway
		cliff : natural=cliff
		mtbroute : osmnetwork[type=route AND @isOneOf(route,mtb)]
		bikeroute : osmnetwork[type=route AND @isOneOf(route,bicycle,lcn,ncn,inline_skates)]
		footroute : osmnetwork[type=route AND (@isOneOf(route,foot,hiking,ski,fitness_trail,walking,running))]
	areas
		landuse : landuse or natural=wood
		building : building
		barrier : barrier
		parking : amenity=parking
		recreation : leisure=stadium or leisure=park or leisure=playground or leisure=garden or leisure=common
		pitch : leisure=pitch
		protected : leisure=nature_reserve or boundary=protected_area or boundary=national_park
		waterarea : natural=water OR waterway
properties
	map-background-color : #fcfaeb
	map-sea-color : #65a4d0 
	text-color : #000000
	text-halo-color : #ffffff
	text-halo-width : 40%
	font-size : 14
	font-family : Clear Sans
	font-weight : bold
	topmost-layer : true 
rules
	target : footroute
		define
			text-color : #cc3333
			min-zoom : 16
		draw : text
	target : place
		for : place=city
			define
				min-zoom : 11
				max-zoom : 14
				font-size : 10:18;13:14
		elsefor : place=suburb
			define
				min-zoom : 15
				max-zoom : 16
		elsefor : place=town
			define
				min-zoom : 11
				max-zoom : 15
				font-size : 10:18;13:14
		elsefor : place=village
			define
				min-zoom : 14
				max-zoom : 16
		elsefor : place=neighbourhood
			define
				min-zoom : 15
				max-zoom : 16
		elsefor : place=locality
			define
				min-zoom : 15
				max-zoom : 17
		elsefor : place=hamlet
			define
				min-zoom : 14
				max-zoom : 17
		elsefor : place=isolated_dwelling
			define
				min-zoom : 15
				max-zoom : 17
		else
			stop
		define
			text-color : #000000
		draw : text
	target : protected			
		define
			min-zoom : 13
			line-style : solid
			line-color : #449944
			line-width : 1
			line-opacity : 0.4
			fill-color : #009900
			fill-hatch : nwse
			fill-opacity : 0.1
			fill-hatch-color : #aaffaa
			fill-hatch-opacity : 0.07
		draw : fill
	target : pitch
		define
			min-zoom : 14
			line-style : solid
			line-color : #aadf79
			line-width : 1
			line-opacity : 0.8
			fill-color : #aadf79
			fill-opacity : 0.3
		draw : fill
	target : building
		define
			line-style : none
			min-zoom : 12
			fill-color : #8c7a6f
		draw : fill
	target : $featuretype(area) 
		for  : landuse=forest or natural=wood
			define
				min-zoom : 13
				max-zoom : 14
				font-size : 16
			draw : text
		elsefor  : leisure=nature_reserve or boundary=protected_area or boundary=national_park
			define
				min-zoom : 13
				max-zoom : 15
				text-halo-color : #eeffee
				text-halo-width : 120%
				text-halo-opacity : 13:0.7;15:0.2
				font-size : 13
			draw : text
		elsefor  : landuse
			define
				min-zoom : 15
				max-zoom : 16
				font-size : 13
			draw : text
		elsefor : leisure=stadium or leisure=park or leisure=playground or leisure=garden or leisure=common
			define
				min-zoom : 17
				font-size : 12
			draw : text
		elsefor : building=yes
			define
				min-zoom : 17
				text-halo-width : 22%
				font-family : arial
				font-size : 12
			draw : text
		else
			define
				min-zoom : 18
				text-halo-width : 22%
				font-family : arial
				font-size : 11
			draw : text
	target : $featuretype(point)
		define
			min-zoom : 18
			text-halo-width : 22%
			font-family : arial
			font-size : 11
		draw : text
	target : $featuretype(line)
		define
			min-zoom : 18
			text-halo-width : 22%
			font-family : arial
			font-size : 11
		draw : text
	target : mtbroute
		define
			min-zoom : 13
			font-size : 15
			text-orientation : flow
			text-color : #770000
		draw : text
		define
			min-zoom : 12
			max-zoom : 12.9
			font-size : 15
			text-orientation : fixed
			text-color : #770000
		draw : text
	target : bikeroute
		define
			min-zoom : 17
			painting-cycle : 4000
			text-color : #0000ff
		draw : text
	target : road
		for : highway=motorway or highway=motorway_link
			define
				min-zoom : 5
				line-color : #ff9155
				line-width : 6:1;11:4;17:10
				border-style : solid
				border-color : #000000
				border-width : 20%
				painting-cycle : 690
		elsefor : highway=trunk or highway=trunk_link
			define
				min-zoom : 5
				line-color : #ff9155
				line-width : 6:1;11:4;17:10
				border-style : solid
				border-color : #000000
				border-width : 20%
				painting-cycle : 690
		elsefor : highway=primary or highway=primary_link
			define
				min-zoom : 5
				line-color : #ff9155
				line-width : 6:1;11:4;17:10
				border-style : solid
				border-color : #000000
				border-width : 20%
				painting-cycle : 690
