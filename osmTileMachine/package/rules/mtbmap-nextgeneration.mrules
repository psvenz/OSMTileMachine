			define
				min-zoom : 10
				line-color : #ffffff
				line-width : 9:1;15:4;18:13
				line-opacity : 10:0;13:1
				fill-opacity : 10:0;13:1
				border-opacity : 10:0;13:1
				border-style : solid
				border-color : #000000
				border-width : 30%
